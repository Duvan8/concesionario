    <div class="allcontain">
        <div class="contact">
            <div class="newslettercontent">
                <div class="leftside">
                    <form class="formulario" action="/insercategoria" method="POST">
                        <div class="contact-form">
                            <h1>REGISTRAR CATEGORIA</h1>
                            <div class="form-group group-coustume">
                                <p>ID</p>
                                <input type="text" name="catid" class="form-control name-form"
                                    placeholder="PLACA DEL VEHICULO">
                                <p>TIPO DE AUTOMOVIL</p>
                                <input type="text" name="catipo" class="form-control email-form" placeholder="ID">
                                <button type="submit" class="btn btn-default btn-submit">REGISTRAR</button>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="google-maps">
                    <div id="googleMap"></div>

                </div>
            </div>

        </div>
