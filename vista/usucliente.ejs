    <div class="allcontain">
        <div class="contact">
            <div class="newslettercontent">
                <div class="leftside">
                    <form class="formulario" action="/insercreditos" method="POST">
                        <div class="contact-form">
                            <div class="form-group group-coustume">
                                <p>PLACA DEL VEHICULO</p>
                                <input type="text" name="vehplaca" class="form-control name-form"
                                    placeholder="PLACA DEL VEHICULO">
                                <p>ID</p>
                                <input type="text" name="datid" class="form-control email-form" placeholder="ID">
                                <p>ID DE CATEGORIA</p>
                                <input type="text" name="catid" class="form-control subject-form"
                                    placeholder="ID DE CATEGORIA">
                                <p>MODELO</p>
                                <input type="text" name="vehmodelo" class="form-control subject-form"
                                    placeholder="MODELO">
                                <p>MARCA</p>
                                <input type="text" name="vehmarca" class="form-control subject-form"
                                    placeholder="MARCA">
                                <p>ESTADO</p>
                                <input type="text" name="vehestado" class="form-control subject-form"
                                    placeholder="ESTADO">
                                <p>PRECIO</p>
                                <input type="text" name="vehprecio" class="form-control subject-form"
                                    placeholder="PRECIO">
                                <button type="submit" class="btn btn-default btn-submit">REGISTRAR</button>
                            </div>
                        </div>
