    <div class="container-fluid" action="/acliente" method="POST">
        <form action="" name="frm" id="frm"></form>
        <table id="tableArticulos" class="table table-striped table-bordered" style="width: 100%;">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>PLACA</th>
                    <th>MODELO</th>
                    <th>MARCA</th>
                    <th>ESTADO</th>
                    <th>PRECIO</th>
                    <th>ELIMINAR</th>
                </tr>
            </thead>
            <tbody>
                <% datos.forEach(function (datos) { %>
                    <tr>
                        <td><input type="text" value="<%= datos.datid %>" name="doccli" id="doc" class="datid"></td>
                        <td><input type="text" value="<%= datos.vehplaca %>" name="doccli" id="doc" class="placa"></td>
                        <td><input type="text" value="<%= datos.vehmodelo %>" name="nomusu" id="nomusu" class="usuid">
                        </td>
                        <td><input type="text" value="<%= datos.vehmarca %>" name="clave" id="clave" class="datnombre">
                        </td>
                        <td><input type="text" value="<%= datos.vehestado %>" name="clave" id="clave"
                                class="datapellido"></td>
                        <td><input type="text" value="<%= datos.vehprecio %>" name="clave" id="clave" class="datipoid">
                        </td>
                        <td><input type="submit" value="Eliminar" name="btn" id="btneli" class="elimcredi"></td>
                    </tr>
                    <% }) %>
            </tbody>
        </table>
        </form>
