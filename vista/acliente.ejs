            </div>
            <nav class="navbar navbar-default midle-nav">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed textcostume" data-toggle="collapse"
                        data-target="#navbarmidle" aria-expanded="false">
                        <h1>SEARCH TEXT</h1>
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
            </nav>
        </div>
    </div>
    <div class="container-fluid" action="/acliente" method="POST">
        <form action="" name="frm" id="frm">
            <table id="tableArticulos" class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>ID USUARIO</th>
                        <th>NOMBRES</th>
                        <th>APELLIDOS</th>
                        <th>TIPO DE IDENTIFICACION</th>
                        <th>NUMERO DE IDENTIFICACION</th>
                        <th>TELEFONO</th>
                        <th>CORREO</th>
                    </tr>
                </thead>
                <tbody>
                    <% datos.forEach(function (datos) { %>
                        <tr>
                            <td><input type="text" value="<%= datos.datid %>" name="doccli" id="doc" class="doc"></td>
                            <td><input type="text" value="<%= datos.usuid%>" name="nomusu" id="nomusu" class="usuid">
                            </td>
                            <td><input type="text" value="<%= datos.datnombre %>" name="clave" id="clave"
                                    class="datnombre">
                            </td>
                            <td><input type="text" value="<%= datos.datapellido %>" name="clave" id="clave"
                                    class="datapellido"></td>
                            <td><input type="text" value="<%= datos.datipoid %>" name="clave" id="clave"
                                    class="datipoid">
                            </td>
                            <td><input type="text" value="<%= datos.datnumeroid %>" name="clave" id="clave"
                                    class="datnumeroid"></td>
                            <td><input type="text" value="<%= datos.datelefono %>" name="clave" id="clave"
                                    class="datelefono"></td>
                            <td><input type="text" value="<%= datos.datcorreo %>" name="clave" id="clave"
                                    class="datcorreo">
                            </td>
                            <td><input type="submit" value="Eliminar" name="btneli"  id="btneli" class="btneli"></td>
                        </tr>
                        <% }) %>
                </tbody>
            </table>
        </form>
    </div>
    <div class="container">
        <div class="leftside">
            <div class="contact-form">
                <form class="formulario" action="/frminsertar" method="POST">
                    <h1>FORMULARIO PERSONAS</h1>
                    <div class="form-group group-coustume">
                        <p>ID</p>
                        <input type="text" name="datid" class="form-control name-form" placeholder="ID">
                        <p>ID USUARIO</p>
                        <input type="text" name="usuid" class="form-control email-form" placeholder="ID USUARIO">
                        <p>NOMBRE</p>
                        <input type="text" name="datnombre" class="form-control subject-form" placeholder="NOMBRE">
                        <p>APELLIDO</p>
                        <input type="text" name="datapellido" class="form-control subject-form" placeholder="APELLIDO">
                        <p>TIPO DE IDENTIFICACION</p>
                        <input type="text" name="datipoid" class="form-control subject-form" placeholder="TIPO DE IDENTIFICACION">
                        <p>NUMERO DE IDENTIFICACION</p>
                        <input type="text" name="datnumeroid" class="form-control subject-form" placeholder="NUMERO DE IDENTIFICACION">
                        <p>TELEFONO</p>
                        <input type="text" name="datelefono" class="form-control subject-form" placeholder="TELEFONO">
                        <p>CORREO</p>
                        <input type="text" name="datcorreo" class="form-control subject-form" placeholder="CORREO">
                        <button type="submit" class="btn btn-default btn-submit">REGISTRAR</button>
                    </div>
                </form>
            </div>
        </div>
    </div>


    <script type="text/javascript" src="source/bootstrap-3.3.6-dist/js/jquery.js"></script>
    <script type="text/javascript" src="source/js/myscript.js"></script>
    <script type="text/javascript" src="source/bootstrap-3.3.6-dist/js/bootstrap.js"></script>

    <script>
        $(window).resize(function () {
            var new_height = $("#image_border").height();
            console.log(new_height);
            $("#googleMap").height(new_height);
        });

        $(window).load(function () {
            var new_height = $("#image_border").height();
            console.log(new_height);
            $("#googleMap").height(new_height);
            google.maps.event.addDomListener(window, 'load', initialize());
        });

    </script>
    <script type="text/javascript" src="source/js/myscript.js"></script>
    <script type="text/javascript" src="source/bootstrap-3.3.6-dist/js/jquery.1.11.js"></script>
</body>

</html>